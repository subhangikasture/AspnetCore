@model BethanysPieShop.Models.ViewModels.PieListViewModel
<!DOCTYPE html>
<head>
	<title>Bethany's Pie Shop</title>
</head>

<body>
	<h1>@ViewBag.CurrentCategory</h1>
	@foreach (var pie in Model.Pies)
	{
		<div>
			<h2>@pie.Name</h2>
			<h3>@pie.Price.ToString("c")</h3>
			<h4>@pie.Category</h4>
		</div>
	}
</body>
</html>